    "node": "^20.2.0"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "css-loader": "^6.7.3",
    "nodemon": "^2.0.4",
    "webpack-dev-server": "^4.15.0",
    "webpack-pwa-manifest": "^4.0.0"
  },
